<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net5.0-windows</TargetFramework>
    <RootNamespace>gsColorearCodigo</RootNamespace>
    <StartupObject>gsColorearCodigo.fColorear</StartupObject>
    <UseWindowsForms>true</UseWindowsForms>

	  <AssemblyName>gsColorearCodigoNET</AssemblyName>

    <Version>1.1.0.0</Version>
    <AssemblyVersion>1.1.0.0</AssemblyVersion>
    <FileVersion>1.1.0.4</FileVersion>
    <Authors>Guillermo Som (Guille)</Authors>
    <Company>elGuille</Company>
    <Description>Utilidad para colorear código y generar código HTML para usar en publicaciones WEB.
Usando gsColorearNET para .NET Standard 2.0.
(para .NET 5.0 RC1 revisión del 19/Sep/2020)</Description>
    <Copyright>Copyright © Guillermo Som (Guille), 2020</Copyright>
    <ApplicationIcon>gsColorearCodigo.ico</ApplicationIcon>
    <OptionStrict>On</OptionStrict>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>elGuille.snk</AssemblyOriginatorKeyFile>
    <PackageReleaseNotes>Usando la nueva versión de gsColorearNET v1.0.0.10</PackageReleaseNotes>

  </PropertyGroup>

  <ItemGroup>
    <None Remove="gsColorearCodigo.ico" />
  </ItemGroup>

  <ItemGroup>
    <Import Include="System.Drawing" />
    <Import Include="System.Windows.Forms" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\gsColorearNET\gsColorearNET.vbproj" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="gsColorearCodigo.ico" />
  </ItemGroup>

